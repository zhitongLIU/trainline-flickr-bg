{
    "name": "trainline-flickr-bg",
    "manifest_version": 2,
    "version": "1",
    "permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "browser_action": {
      "default_popup": "frontend/popup.html"
    },
    "content_scripts": [{
      "matches": ["https://www.thetrainline.com/"],
      "js": ["jquery-3.2.1.min.js", "backend/app.js"]
    }]
}
